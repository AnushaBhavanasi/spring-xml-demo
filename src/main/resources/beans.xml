<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                       http://www.springframework.org/schema/context
                       http://www.springframework.org/schema/context/spring-context-3.0.xsd">


    <bean id="actor" class="com.stackroute.domain.Actor">
        <constructor-arg name="actorName" value="Latika"/>
        <constructor-arg name="actorGender" value="Female"/>
        <constructor-arg name="actorAge" value="21"/>
    </bean>

    <bean id="movies" autowire="byName" class="com.stackroute.domain.Movie">
    </bean>
    <bean id="movie"  class="com.stackroute.domain.Movie">
        <constructor-arg ref="actor"/>
    </bean>
    <bean id="applicationcontextAwareDemo" class="com.stackroute.awareinterface.ApplicationContextAwareDemo">

    </bean>
    <bean id="beanFactory" class="com.stackroute.awareinterface.BeanFactoryAwareDemo">

    </bean>
    <bean id="beanName" class="com.stackroute.awareinterface.BeanNameAwareDemo">

    </bean>
    <bean id="resourceLoader" class="com.stackroute.awareinterface.ResourceLoaderAwareDemo">

    </bean>
</beans>